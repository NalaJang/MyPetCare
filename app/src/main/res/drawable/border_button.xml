<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp" >

        <shape android:shape="rectangle" >

            <!-- 테두리의 두께와 색상 -->
            <stroke
                android:width="1dp"
                android:color="#F3581F" />
            <!-- 배경색 -->
            <solid android:color="#fff" />

            <corners android:radius="20dp" />
        </shape>
    </item>
</layer-list>